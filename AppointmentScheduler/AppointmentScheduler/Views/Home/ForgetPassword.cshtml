@model AppointmentScheduler.Models.LoggedinInfo
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Change Password";
}
<h1>@ViewBag.Title</h1>
<h2>@ViewBag.Message</h2>
<div>
    @using (Html.BeginForm("ForgetPassword", "Home", FormMethod.Post))
    {
        <table>
            <tr>
                <td>@Html.Label("User Name")</td>
                <td>
                    @Html.TextBoxFor(m => m.UserName)
                    @Html.ValidationMessage("UserName", new { @class = "btn-danger" })
                </td>
            </tr>
            <tr>
                <td>@Html.Label("New Password")</td>
                <td>@Html.Password("Password")</td>
            </tr>
            <tr>
                <td>
                    <input type="submit" value="Submit" />
                </td>
            </tr>
        </table>
     }
</div>


